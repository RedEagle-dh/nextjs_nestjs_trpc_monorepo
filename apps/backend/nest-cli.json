{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"deleteOutDir": true,
		"builder": "tsc"
	},
	"projects": {
		"backend": {
			"type": "application",
			"root": ".",
			"entryFile": "main",
			"sourceRoot": "src",
			"compilerOptions": {
				"tsConfigPath": "tsconfig.build.json",
				"builder": "tsc"
			}
		}
	}
}
